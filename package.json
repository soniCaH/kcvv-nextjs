{
  "name": "kcvv-nextjs",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "lint": "eslint .",
    "lint:fix": "eslint --fix .",
    "type-check": "tsc --noEmit",
    "test": "vitest run",
    "test:watch": "vitest",
    "test:ui": "vitest --ui",
    "test:coverage": "vitest run --coverage",
    "storybook": "storybook dev -p 6006",
    "build-storybook": "storybook build",
    "check-all": "npm run lint && npm run type-check && npm run test && npm run build",
    "build:responsibility": "tsx scripts/generate-responsibility-data.ts",
    "watch:responsibility": "tsx watch scripts/generate-responsibility-data.ts",
    "migration:status": "node scripts/migration-status.js",
    "migration:create": "node scripts/migration-create.js",
    "prepare": "husky"
  },
  "dependencies": {
    "@radix-ui/react-accordion": "1.2.12",
    "@radix-ui/react-dialog": "1.1.15",
    "@radix-ui/react-dropdown-menu": "2.1.16",
    "@radix-ui/react-tabs": "1.1.13",
    "clsx": "2.1.1",
    "d3-org-chart": "3.1.1",
    "effect": "3.19.14",
    "embla-carousel-react": "8.6.0",
    "lucide-react": "0.562.0",
    "luxon": "3.7.2",
    "motion": "12.26.2",
    "next": "16.1.3",
    "react": "19.2.3",
    "react-dom": "19.2.3",
    "react-share": "5.2.2",
    "tailwind-merge": "3.4.0"
  },
  "devDependencies": {
    "@chromatic-com/storybook": "5.0.0",
    "@commitlint/cli": "20.3.1",
    "@commitlint/config-conventional": "20.3.1",
    "@storybook/addon-a11y": "10.1.11",
    "@storybook/addon-coverage": "3.0.0",
    "@storybook/addon-docs": "10.1.11",
    "@storybook/addon-mcp": "0.1.8",
    "@storybook/addon-onboarding": "10.1.11",
    "@storybook/addon-vitest": "10.1.11",
    "@storybook/nextjs-vite": "10.1.11",
    "@tailwindcss/postcss": "4.1.18",
    "@testing-library/jest-dom": "6.9.1",
    "@testing-library/react": "16.3.1",
    "@testing-library/user-event": "14.6.1",
    "@types/d3-org-chart": "3.1.2",
    "@types/luxon": "3.7.1",
    "@types/node": "24.10.9",
    "@types/react": "19.2.8",
    "@types/react-dom": "19.2.3",
    "@vitejs/plugin-react": "5.1.2",
    "@vitest/coverage-v8": "4.0.17",
    "@vitest/ui": "4.0.17",
    "eslint": "9.39.2",
    "eslint-config-next": "16.1.3",
    "eslint-plugin-storybook": "10.1.11",
    "gray-matter": "4.0.3",
    "happy-dom": "20.3.1",
    "husky": "9.1.7",
    "lint-staged": "16.2.7",
    "prettier": "3.8.0",
    "prettier-plugin-tailwindcss": "0.7.2",
    "remark": "15.0.1",
    "remark-parse": "11.0.0",
    "storybook": "10.1.11",
    "tailwindcss": "4.1.18",
    "tsx": "4.21.0",
    "typescript": "5.9.3",
    "unist-util-visit": "5.0.0",
    "vitest": "4.0.17"
  },
  "overrides": {
    "@types/react": "19.2.8",
    "@types/react-dom": "19.2.3",
    "typescript": "5.9.3"
  },
  "lint-staged": {
    "*.{js,jsx,ts,tsx}": [
      "eslint --fix",
      "prettier --write",
      "vitest related --run"
    ],
    "*.{json,md,yml,yaml}": [
      "prettier --write"
    ]
  }
